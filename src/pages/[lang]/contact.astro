---
import { useTranslations } from '../../i18n/utils';

export function getStaticPaths() {
  return [
    { params: { lang: "es" }},
    { params: { lang: "en" }},
  ];
}


const { lang } = Astro.params as any;
const t = useTranslations(lang);

import Layout from '../../layouts/Layout.astro';
---

<Layout >
  <h1>{t('contact.title')}</h1>
</Layout>